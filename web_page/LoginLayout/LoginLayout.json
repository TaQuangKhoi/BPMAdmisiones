    "applicationContainerClasses" : {
      "type" : "expression",
      "value" : [
        "return function (currentApp){",
        "    var cssClasses = \"app-item \";",
        "    ",
        "    if(currentApp.token === $data.appToken){",
        "        cssClasses += \"app-item--current\";",
        "    }",
        "    ",
        "    return cssClasses;",
        "};"
      ],
      "exposed" : false
    },
    "eventList" : {
      "type" : "json",
      "value" : [
        "{\"actions\":[]}"
      ],
      "exposed" : false
    },

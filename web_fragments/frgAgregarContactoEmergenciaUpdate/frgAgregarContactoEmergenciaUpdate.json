    "addParentesco" : {
      "type" : "expression",
      "value" : [
        "var mostrar = true;\r",
        "for (var x = 0; x < $data.catParentesco.length; x++) {\r",
        "    if ($data.casoEmergenciaJSON.catParentesco === undefined) {\r",
        "        mostrar = true;\r",
        "    } else {\r",
        "        if ($data.catParentesco[x].persistenceId === $data.casoEmergenciaJSON.catParentesco.persistenceId) {\r",
        "            if ($data.catParentesco[x].descripcion === \"Otro\") {\r",
        "                mostrar = false;\r",
        "                $data.showmadrealert = false;\r",
        "                $data.showpadrealert = false;\r",
        "            } else if($data.catParentesco[x].descripcion === \"Padre\"){\r",
        "                if(!$data.padrevive){\r",
        "                    $data.showpadrealert = true;\r",
        "                    $data.showmadrealert = false;\r",
        "                    $data.casoEmergenciaJSON.catParentesco = undefined;\r",
        "                }\r",
        "                \r",
        "            } else {\r",
        "                $data.showmadrealert = false;\r",
        "                $data.showpadrealert = false;\r",
        "                mostrar = true;\r",
        "            }\r",
        "        }\r",
        "    }\r",
        "\r",
        "}\r",
        "\r",
        "return mostrar;"
      ],
      "exposed" : false
    },
    "datosPadre" : {
      "type" : "expression",
      "value" : [
        "debugger;\r",
        "var mostrar = false;\r",
        "var otro = true;\r",
        "for (var x = 0; x < $data.catCasoDeEmergencia.length; x++) {\r",

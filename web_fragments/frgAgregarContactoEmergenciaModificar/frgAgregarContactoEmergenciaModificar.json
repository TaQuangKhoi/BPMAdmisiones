        "            }\r",
        "            $data.showalertmadre = \"false\";\r",
        "            $data.showalertpadre = \"true\";\r",
        "\r",
        "        } else if ($data.catCasoDeEmergencia[x].descripcion === \"Tutor\") {\r",
        "            $data.showalertpadre = \"true\";\r",
        "            $data.showalertmadre = \"true\";\r",
        "            $data.casoEmergenciaJSON.nombre = $data.tutorbdm[0].nombre + \" \" + $data.tutorbdm[0].apellidos;\r",
        "            $data.casoEmergenciaJSON.telefono = $data.tutorbdm[0].telefono;\r",
        "            $data.casoEmergenciaJSON.catParentesco = $data.tutorbdm[0].catParentezco;\r",
        "            if($data.tutorbdm[0].otroParentesco !== \"\"){\r",
        "                $data.casoEmergenciaJSON.parentesco = $data.tutorbdm[0].otroParentesco;\r",
        "            }else{\r",
        "                $data.casoEmergenciaJSON.parentesco = $data.tutorbdm[0].catParentezco.descripcion;\r",
        "            }\r",
        "        } else if ($data.catCasoDeEmergencia[x].descripcion === \"Otro\") {\r",
        "            $data.contactoJson = {\r",
        "              \"nombre\": \"\",\r",
        "              \"telefono\": \"\",\r",
        "              \"catCasoDeEmergencia\": null,\r",
        "              \"telefonoCelular\": \"\",\r",
